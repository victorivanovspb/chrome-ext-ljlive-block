{
  "name"        : "LjliveBlock",
  "description" : "Blocks Livejournal's div#ljlive element.",
  "version"     : "0.2",
  "manifest_version" : 2,
  "icons" : {
    "16"  : "images/b-48.png",
    "48"  : "images/b-48.png",
    "128" : "images/b-128.png"
  },
  "content_scripts" : [{
    "matches" : ["http://*.livejournal.com/*", "http://*.livejournal.com/*.html"],
    "css"     : ["css/basic.css"],
    "js"      : ["lib/jquery-1.7.min.js", "contentscript.js"]
  }]
}